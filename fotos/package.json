{
  "name": "fotos",
  "version": "1.0.0",
  "main": "public/electron.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"cross-env DEV= tsc-watch -p tsconfig-electron.json --onSuccess \\\"electron .\\\"\"",
    "build": "electron-builder",
    "preview": "vite build && tsc -p tsconfig-electron.json && cross-env PREVIEW= electron .",
    "package": "vite build && tsc -p tsconfig-electron.json && electron-packager . \"fotos\" --platform=win32 --arch=x64 --out=release --overwrite --icon=public/icon.ico --asar --extra-resource=.env --win32metadata.ConsoleApplication=false"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.8.2",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/vite": "^4.1.7",
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.9.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.6.1",
    "clsx": "^2.1.1",
    "dexie": "^4.0.11",
    "dns": "^0.2.2",
    "dot-env": "^0.0.1",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.12.1",
    "fs-extra": "^11.2.0",
    "ftp-srv": "^4.6.3",
    "idb": "^8.0.3",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.511.0",
    "next-themes": "^0.4.6",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.3",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.6.0",
    "sonner": "^2.0.3",
    "sqlite3": "^5.1.7",
    "swagger-ui-react": "^5.17.14",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "workerpool": "^9.2.0",
    "ws": "^8.17.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.0",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.8.3",
    "vite": "^6.0.1",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.fotos.app",
    "productName": "fotos",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json",
      ".env"
    ],
    "asar": true,
    "win": {
      "target": "portable",
      "icon": "public/icon.ico"
    }
  }
}
